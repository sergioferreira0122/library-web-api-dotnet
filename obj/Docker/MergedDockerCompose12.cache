name: dockercompose4766360516660457190
services:
  app_db:
    container_name: db-api-library
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: password12345
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      library.api: null
    ports:
      - mode: ingress
        target: 1433
        published: "8002"
        protocol: tcp
  library.api:
    build:
      context: C:\Users\Sergio\source\repos\Library
      dockerfile: Library/Dockerfile
    container_name: web-api-library
    depends_on:
      app_db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      DB_HOST: webapidb
      DB_NAME: LibraryAPI
      DB_SA_PASSWORD: password12345
    image: libraryapi
    networks:
      library.api: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 80
        published: "8001"
        protocol: tcp
networks:
  library.api:
    name: dockercompose4766360516660457190_library.api