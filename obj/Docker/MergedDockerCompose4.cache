name: dockercompose4766360516660457190
services:
  library.api:
    build:
      context: C:\Users\Sergio\source\repos\Library
      dockerfile: Library/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
    image: libraryapi
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
  livro-app:
    build:
      context: C:\Users\Sergio\source\repos\Library
      dockerfile: Dockerfile
    environment:
      Database: LibraryDb
      DbPort: "1433"
      DbServer: mssql-server
      DbUser: SA
      Password: password123
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "8090"
        protocol: tcp
  mssql-server:
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Express
      SA_PASSWORD: password123
    image: mcr.microsoft.com/mssql/server:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\dados\volumes\sqlserver
        target: /var/opt/mssql/data
        bind:
          create_host_path: true
