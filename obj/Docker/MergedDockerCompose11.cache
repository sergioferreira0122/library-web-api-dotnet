        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Sergio\.nuget\packages\
        target: /root/.nuget/packages
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Sergio\AppData\Roaming/ASP.NET/Https
